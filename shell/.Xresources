!Droid Sans Mono
URxvt.font: xft:Ubuntu Mono derivative Powerline:pixelsize=18,style=regular
URxvt.letterSpace: -1

urxvt*depth: 32
urxvt*background: rgba:1d00/1f00/2100/eeee

URxvt.saveLines: 2000
URxvt.scrollBar: False
URxvt.scrollstyle: rxvt
URxvt.termName: xterm-256color
URxvt.urgentOnBell: True
URxvt.cursorBlink: False
URxvt.intensityStyles:	false

!URxvt.background:       [70]#000000
URxvt.depth:		32
URxvt.scrollBar:	false
URxvt.cursorColor:	white
URxvt.perl-ext-common:	default,matcher,resize-font
URxvt.perl-lib:		$HOME/.config/Scripts
URxvt.url-launcher:	/usr/bin/xdg-open
URxvt.matcher.button:	1
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
urxvt*background: rgba:1d00/1f00/2100/ffff

URxvt.colorUL:		#4682B4
URxvt.font: 		xft:monospace:size=11
#URxvt.font: xft:Ubuntu Mono:pixelsize=12,style=regular
#Xterm.font: xft:Ubuntu Mono:pixelsize=18,style=regular
URxvt.letterSpace: -1

URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~

